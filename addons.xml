<addon id="repository.chimkentec"
       name="chimkentec Add-ons"
	   version="1.0.2"
	   provider-name="TDW1980">
<requires>
  <import addon="xbmc.addon" version="12.0.0"/>
</requires>
	<extension point="xbmc.addon.repository" name="KodiE">
		<info compressed="false">https://github.com/chimkentec/KodiE/raw/master/addons.xml</info>
		<checksum>https://github.com/chimkentec/KodiE/raw/master/addons.xml.md5</checksum>
		<datadir zip="true">https://github.com/chimkentec/KodiE/raw/master/</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>Install Add-ons from chimkentec Add-on Repository</summary>
		<description>Plug-ins for XBMC</description>
		<disclaimer>There are git my add-ons for XBMC</disclaimer>
		<platform>all</platform>
	</extension>
</addon>
</addons>
